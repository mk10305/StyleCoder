<h1>Listing wore_it_votes</h1>

<table>
  <thead>
    <tr>
      <th>Vote</th>
      <th>User</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wore_it_votes.each do |wore_it_vote| %>
      <tr>
        <td><%= wore_it_vote.vote %></td>
        <td><%= wore_it_vote.user_id %></td>
        <td><%= wore_it_vote.item_id %></td>
        <td><%= link_to 'Show', wore_it_vote %></td>
        <td><%= link_to 'Edit', edit_wore_it_vote_path(wore_it_vote) %></td>
        <td><%= link_to 'Destroy', wore_it_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wore it vote', new_wore_it_vote_path %>
